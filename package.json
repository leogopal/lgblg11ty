		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/leogopal/lgblg11ty.git"
	},
	"author": {
		"name": "Leo Gopal",
		"email": "leo@leogopal.com",
		"url": "https://leogopal.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=14"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/leogopal/lgblg11ty/issues"
	},
	"homepage": "https://github.com/leogopal/lgblg11ty#readme",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0",
		"@11ty/eleventy-img": "^3.0.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "^1.0.1",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
		"luxon": "^3.2.1",
		"markdown-it-anchor": "^8.6.6"
	},
	"dependencies": {
		"serve": "^14.2.0"
	}
